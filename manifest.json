{
  "manifest_version": 2,
  "name": "GitHub++",
  "description": "Adds keyboard shortcuts and other stuff to GitHub.",
  "version": "0.0.1",
  "author": "Jeffrey Wear",
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": [
      "src/globals.js",
      "src/app.js"
    ],
    "run_at": "document_idle"
  }],
  "web_accessible_resources": [
    "assets/Basso.wav",
    "src/content.js"
  ]
}
